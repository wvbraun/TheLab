package games.ticTacToe.view.graphical;

import java.awt.Rectangle;

import games.ticTacToe.board.Tile;

import javax.swing.JButton;

@SuppressWarnings("serial")
public class BoardTile extends JButton
{
	private int x, y, radius;
	private Tile tile;
	
	private Rectangle rec;
	
	public BoardTile(int x, int y, int radius, Tile tile)
	{
		this.x      = x;
		this.y      = y;
		this.tile   = tile;
		this.radius = radius;
	}

}
