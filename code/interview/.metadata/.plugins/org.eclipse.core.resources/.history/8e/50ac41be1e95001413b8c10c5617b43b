package code.interview.general;

public class General
{

	public boolean isRotated(int[] ls1, int[] ls2)
	{
		
	}
	
	private boolean isSame(int[] ls1, int[] ls2)
	{
		boolean result = true;
		
		for (int i = 0; i < ls1.length; ++i)
		{
			if (ls1[i] != ls2[i])
			{
				result = false;
				break;
			}
		}
			
		return result;
	}
}
