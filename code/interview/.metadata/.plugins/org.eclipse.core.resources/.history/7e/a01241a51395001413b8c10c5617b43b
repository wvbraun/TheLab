package code.interview.general;

import java.util.ArrayList;

public class Array 
{
	ArrayList<Integer> xs;
	
	public Array(ArrayList<Integer> xs)
	{
		this.xs = xs;
	}
	
	public int getMax()
	{
		int max = xs.get(0);
		
		for (int i = 1; i < xs.size(); ++i)
		{
			if (xs.get(i) > max)
			{
				max = xs.get(i);
			}
		}
		
		return max;
	}
	
	public findMostFrequent()
	{
		Array lookup = new Array(new ArrayList<Integer>());
		lookup.setAllZero();
		
		for (int x : xs)
		{
			++x;
		}
		
		int index = getMax();
	}

}
