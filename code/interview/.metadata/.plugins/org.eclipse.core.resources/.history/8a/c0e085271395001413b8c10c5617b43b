package code.interview.general;

public class Array 
{
	int[] xs;
	
	public Array(int[] xs)
	{
		this.xs = xs;
	}
	
	public void setAllZero()
	{
		for (int x : xs)
		{
			x = 0;
		}
	}
	
	public int getMax()
	{
		int max = xs[0];
		
		for (int i = 1; i < xs.length; ++i)
		{
			if (xs[i] > max)
			{
				max = xs[i];
			}
		}
		
		return max;
	}
	
	public findMostFrequent()
	{
		Array lookup = new Array(new int[(int) Math.pow(2, 32)]); // 2^32 is the largest number we could have.
		lookup.setAllZero();
		
		for (int x : xs)
		{
			++x;
		}
		
		int index = getMax();
	}

}
